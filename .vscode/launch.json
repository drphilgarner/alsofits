   "version": "0.3.0",
    "configurations": [
        {
            "name": "PartsCategoriesImporter",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/PartsCategoriesImporter/bin/Debug/netcoreapp3.0/PartsCategoriesImporter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/PartsCategoriesImporter",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": "MakesImporter",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/MakesImporter/bin/Debug/netcoreapp3.0/MakesImporter.dll",
            "args": [],
            "cwd": "${workspaceFolder}/MakesImporter",
            // For more information about the 'console' field, see https://aka.ms/VSCode-CS-LaunchJson-Console
            "console": "internalConsole",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}